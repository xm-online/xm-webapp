  "scripts": {
    "ng": "ng",
    "xm-ngx": "xm-ngx",
    "prestart": "npm run prebuild",
    "start": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve",
    "prestart:prod": "npm run prebuild",
    "start:prod": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve --configuration=serve-production",
    "start-local": "npm run prebuild && ng serve --proxy-config local.proxy.conf.js --live-reload",
    "start-local:aot": " npm run prebuild && node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve --proxy-config local.proxy.conf.js --live-reload  --host=0.0.0.0 --configuration production --aot=true",
    "dev": "npm run prebuild && node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve --proxy-config local.proxy.conf.js --live-reload --host=0.0.0.0",
    "dev-idp": "npm run prebuild && node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng serve --proxy-config local.proxy.conf.js --live-reload --host=0.0.0.0 --disable-host-check  -c idp",
    "dev:aot": "npm run prebuild && ng serve --proxy-config local.proxy.conf.js --live-reload  --host=0.0.0.0 --configuration production --aot=true",
    "prebuild": "xm-ngx ext-install && xm-ngx ext-lazy-module && xm-ngx ext-assets && xm-ngx ext-i18n && xm-ngx ext-theming && xm-ngx ext-themes",
    "build": "ng build --stats-json",
    "prebuild:prod": "npm run prebuild",
    "build:prod": "ng build --configuration production",
    "build-aot": "npm run prebuild && node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --configuration production --aot=true",
    "build-no-aot": "npm run prebuild && ng build --configuration production --aot=false",
    "build-prod-v1": "npm run prebuild && ng build -pr --configuration production",
    "build-prod-v1-stat": "npm run prebuild && ng build -pr --configuration production --statsJson",
    "build-prod-v2": "npm run prebuild && node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --configuration production --aot=true --build-optimizer=true --vendor-chunk=true --common-chunk=true",
    "build-prod-v2-stat": "npm run prebuild && node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --configuration production --aot=true --build-optimizer=true --vendor-chunk=true --common-chunk=true --statsJson",
    "test": "ng test --source-map=false",
    "test:prod": "ng test --code-coverage=true --watch=false --browsers=ChromeHeadlessNoSandbox",
    "e2e": "ng e2e",
    "lint": "npm run lint:js",
    "lint:js": "node --max_old_space_size=4096 ./node_modules/eslint/bin/eslint.js . --ext .ts --quiet",
    "lint:sass": "stylelint */**/*.scss --quiet",
    "docs": "compodoc -p tsconfig.json",
    "get-unwraped-labels": "node ./scripts/get-unwraped-labels.js",
    "get-translations": "node ./scripts/get-translations.js",
    "translations": "npm run get-translations && xm-ngx ext-i18n",
    "report:bundle": "webpack-bundle-analyzer dist/stats.json",
    "get-version": "printf $npm_package_version",
    "postinstall": "node ./scripts/build-packages.js && ngcc --properties esm5 browser module main"
  },
