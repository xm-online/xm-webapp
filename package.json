{
  "name": "xm-webapp",
  "version": "4.0.83",
  "release": "4.0.0",
  "private": true,
  "description": "Xm-webapp",
  "homepage": "https://github.com/xm-online/xm-webapp",
  "bugs": "https://github.com/xm-online/xm-webapp/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/xm-online/xm-webapp.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=16.13.0",
    "npm": ">=8.1.0",
    "yarn": ">=1.22.4 <2"
  },
  "workspaces": [
    "packages/*",
    "src/app/ext/*"
  ],
  "scripts": {
    "ng": "ng",
    "xm-ngx": "xm-ngx",
    "postinstall": "node ./scripts/build-packages.js && ngcc --properties esm5 browser module main",
    "prebuild": "xm-ngx ext-lazy-module && xm-ngx ext-assets && xm-ngx ext-i18n && xm-ngx ext-theming && xm-ngx ext-themes && xm-ngx dynamic-specification && npm run import-jsf-exts",
    "start": "npm run prebuild && ng serve",
    "start:prod": "npm run prebuild && ng serve --configuration=serve-production",
    "start-local": "npm run prebuild && ng serve --proxy-config local.proxy.conf.js --live-reload",
    "start-local:aot": " npm run prebuild && ng serve --proxy-config local.proxy.conf.js --live-reload  --host=0.0.0.0 --configuration production --aot=true",
    "dev": "npm run prebuild && ng serve --proxy-config local.proxy.conf.js --live-reload --host=0.0.0.0",
    "dev-idp": "npm run prebuild && ng serve --proxy-config local.proxy.conf.js --live-reload --host=0.0.0.0 --disable-host-check  -c idp",
    "dev:aot": "npm run prebuild && ng serve --proxy-config local.proxy.conf.js --live-reload  --host=0.0.0.0 --configuration production --aot=true",
    "build": "ng build --stats-json",
    "build:prod": "npm run prebuild && ng build --configuration production",
    "build-aot": "npm run prebuild && ng build --configuration production --aot=true",
    "build-no-aot": "npm run prebuild && ng build --configuration production --aot=false",
    "build-prod-v1": "npm run prebuild && ng build -pr --configuration production",
    "build-prod-v1-stat": "npm run prebuild && ng build -pr --configuration production --statsJson",
    "build-prod-v2": "npm run prebuild && ng build --configuration production --aot=true --build-optimizer=true --vendor-chunk=true --common-chunk=true",
    "build-prod-v2-stat": "npm run prebuild && ng build --configuration production --aot=true --build-optimizer=true --vendor-chunk=true --common-chunk=true --statsJson",
    "test": "ng test --source-map=false",
    "test:prod": "ng test --source-map=false --code-coverage=true --no-progress --watch=false --browsers=ChromeHeadlessNoSandbox",
    "e2e": "ng e2e",
    "lint": "npm run lint:js",
    "lint:js": "eslint . --ext .ts --quiet",
    "lint:sass": "stylelint */**/*.scss --quiet",
    "docs": "compodoc -p tsconfig.json",
    "get-unwraped-labels": "node ./scripts/get-unwraped-labels.js",
    "get-translations": "node ./scripts/get-translations.js",
    "import-jsf-exts": "node ./scripts/jsf-ext-importer.js",
    "translations": "npm run get-translations && xm-ngx ext-i18n",
    "report:bundle": "webpack-bundle-analyzer dist/stats.json",
    "get-version": "printf $npm_package_version",
    "storybook": "ng run xm-webapp:storybook",
    "build-storybook": "ng run xm-webapp:build-storybook"
  },
