{
  "compileOnSave": true,
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": false,
    "strict": false,
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "baseUrl": "./",
    "sourceMap": true,
    "declaration": true,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "ES2022",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@xm-ngx/administration/*": [
        "packages/administration/src/*"
      ],
      "@xm-ngx/components/*": [
        "packages/components/*"
      ],
      "@xm-ngx/core/environment": [
        "src/environments"
      ],
      "@xm-ngx/ext/*": [
        "src/app/ext/*"
      ],
      "@xm-ngx/confirm-dialog": [
        "node_modules/@xm-ngx/components/confirm-dialog"
      ],
      "@xm-ngx/json-schema-form": [
        "node_modules/@xm-ngx/json-schema-form"
      ],
      "@xm-ngx/json-schema-form/core": [
        "node_modules/@xm-ngx/json-schema-form"
      ],
      "@xm-ngx/json-schema-form/components": [
        "node_modules/@xm-ngx/json-schema-form"
      ],
      "@xm-ngx/json-schema-form/components/*": [
        "node_modules/@xm-ngx/json-schema-form"
      ],
      "@xm-ngx/timeline": [
        "packages/timeline"
      ],
      "@xm-ngx/timeline/*": [
        "packages/timeline/*"
      ],
      "@xm-ngx/shared": [
        "node_modules/@xm-ngx/shared"
      ],
      "@xm-ngx/shared/exceptions": [
        "node_modules/@xm-ngx/exceptions"
      ],
      "@xm-ngx/shared/validators": [
        "node_modules/@xm-ngx/validators"
      ],
      "@xm-ngx/shared/interfaces": [
        "node_modules/@xm-ngx/interfaces"
      ],
      "@xm-ngx/shared/operators": [
        "node_modules/@xm-ngx/operators"
      ],
      "@xm-ngx/shared/pipes": [
        "node_modules/@xm-ngx/pipes"
      ],
      "@xm-ngx/account": [
        "packages/account"
      ],
      "@xm-ngx/account/*": [
        "packages/account/*"
      ],
      "@xm-ngx/components/entity-collection": [
        "node_modules/@xm-ngx/repositories"
      ],
      "@xm-ngx/entity": [
        "packages/entity"
      ],
      "@xm-ngx/entity/pipes": [
        "packages/entity/pipes"
      ],
      "@xm-ngx/entity/*": [
        "packages/entity/src/*"
      ],
      "@xm-ngx/dashboard": [
        "packages/dashboard"
      ],
      "@xm-ngx/dashboard/testing": [
        "packages/dashboard/testing"
      ],
      "@xm-ngx/balance": [
        "packages/balance"
      ],
      "@xm-ngx/balance/*": [
        "packages/balance/src/*"
      ],
      "rxjs": [
        "node_modules/rxjs"
      ],
      "rxjs/*": [
        "node_modules/rxjs/*"
      ],
      "@angular/*": [
        "node_modules/@angular/*"
      ]
    },
    "inlineSources": true,
    "module": "es2020",
    "stripInternal": false,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "newLine": "lf",
    "strictFunctionTypes": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "useDefineForClassFields": false
  }
}
