{
  "compileOnSave": true,
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": false,
    "strict": false,
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "baseUrl": "./",
    "sourceMap": true,
    "declaration": true,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "ES2022",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@xm-ngx/interfaces": ["packages/interfaces"],
      "@xm-ngx/exceptions":["packages/exceptions"],
      "@xm-ngx/pipes": ["packages/pipes"],
      "@xm-ngx/validators":["packages/validators"],
      "@xm-ngx/operators": ["packages/operators"],
      "@xm-ngx/eslint":["packages/eslint"],
      "@xm-ngx/repositories": ["packages/repositories"],
      "@xm-ngx/core":["packages/core"],

      "@xm-ngx/core/auth": ["packages/core/auth"],
      "@xm-ngx/core/client":["packages/core/client"],
      "@xm-ngx/core/config": ["packages/core/config"],
      "@xm-ngx/core/context":["packages/core/context"],
      "@xm-ngx/core/entity": ["packages/core/entity"],
      "@xm-ngx/core/permission":["packages/core/permission"],
      "@xm-ngx/core/role": ["packages/core/role"],
      "@xm-ngx/core/theme": ["packages/core/theme"],
      "@xm-ngx/core/user":["packages/core/user"],
      "@xm-ngx/core/dashboard":["packages/core/dashboard"],

      "@xm-ngx/dynamic": ["packages/dynamic"],
      "@xm-ngx/dynamic/public": ["packages/dynamic/public"],
      "@xm-ngx/dynamic/route": ["packages/dynamic/route"],
      "@xm-ngx/logger":["packages/logger"],
      "@xm-ngx/translation": ["packages/translation"],
      "@xm-ngx/translation/pipes": ["packages/translation/pipes"],
      "@xm-ngx/translation/testing": ["packages/translation/testing"],
      "@xm-ngx/toaster":["packages/toaster"],
      "@xm-ngx/alert": ["packages/alert"],

      "@xm-ngx/administration/*": [
        "packages/administration/src/*"
      ],
      "@xm-ngx/components/*": [
        "packages/components/*"
      ],
      "@xm-ngx/core/environment": [
        "src/environments"
      ],
      "@xm-ngx/ext/*": [
        "src/app/ext/*"
      ],
      "@xm-ngx/confirm-dialog": [
        "packages/confirm-dialog"
      ],
      "@xm-ngx/json-schema-form": [
        "packages/json-schema-form/core"
      ],
      "@xm-ngx/json-schema-form/core": [
        "packages/json-schema-form"
      ],
      "@xm-ngx/json-schema-form/components": [
        "packages/json-schema-form"
      ],
      "@xm-ngx/json-schema-form/components/*": [
        "packages/json-schema-form/components/*"
      ],
      "@xm-ngx/timeline": [
        "packages/timeline"
      ],
      "@xm-ngx/timeline/*": [
        "packages/timeline/*"
      ],
      "@xm-ngx/shared": [
        "packages/shared"
      ],
      "@xm-ngx/shared/exceptions": [
        "packages/exceptions"
      ],
      "@xm-ngx/shared/validators": [
        "packages/validators"
      ],
      "@xm-ngx/shared/interfaces": [
        "packages/interfaces"
      ],
      "@xm-ngx/shared/operators": [
        "packages/operators"
      ],
      "@xm-ngx/shared/pipes": [
        "packages/pipes"
      ],
      "@xm-ngx/account": [
        "packages/account"
      ],
      "@xm-ngx/account/*": [
        "packages/account/*"
      ],
      "@xm-ngx/components/entity-collection": [
        "packages/repositories"
      ],
      "@xm-ngx/entity": [
        "packages/entity"
      ],
      "@xm-ngx/entity/pipes": [
        "packages/entity/pipes"
      ],
      "@xm-ngx/entity/*": [
        "packages/entity/src/*"
      ],
      "@xm-ngx/dashboard": [
        "packages/dashboard"
      ],
      "@xm-ngx/dashboard/testing": [
        "packages/dashboard/testing"
      ],
      "@xm-ngx/balance": [
        "packages/balance"
      ],
      "@xm-ngx/balance/*": [
        "packages/balance/src/*"
      ],
      "rxjs": [
        "node_modules/rxjs"
      ],
      "rxjs/*": [
        "node_modules/rxjs/*"
      ],
      "@angular/*": [
        "node_modules/@angular/*"
      ]
    },
    "inlineSources": true,
    "module": "es2020",
    "stripInternal": false,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "newLine": "lf",
    "strictFunctionTypes": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "useDefineForClassFields": false
  }
}
