{
  "compileOnSave": true,
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": false,
    "strict": false,
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "baseUrl": "./",
    "sourceMap": true,
    "declaration": true,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "es2020",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@xm-ngx/styles": [
        "packages/styles"
      ],
      "@xm-ngx/administration/*": [
        "packages/administration/src/*"
      ],
      "@xm-ngx/components/*": [
        "packages/components/*"
      ],
      "@xm-ngx/core": [
        "packages/core"
      ],
      "@xm-ngx/core/auth": [
        "packages/core/auth"
      ],
      "@xm-ngx/core/auth/testing": [
        "packages/core/auth/testing"
      ],
      "@xm-ngx/core/config": [
        "packages/core/config"
      ],
      "@xm-ngx/core/config/testing": [
        "packages/core/config/testing"
      ],
      "@xm-ngx/core/user": [
        "packages/core/user"
      ],
      "@xm-ngx/core/user/testing": [
        "packages/core/user/testing"
      ],
      "@xm-ngx/core/client": [
        "packages/core/client"
      ],
      "@xm-ngx/core/global-error-handler": [
        "packages/core/global-error-handler"
      ],
      "@xm-ngx/core/permission": [
        "packages/core/permission"
      ],
      "@xm-ngx/core/permission/testing": [
        "packages/core/permission/testing"
      ],
      "@xm-ngx/core/theme": [
        "packages/core/theme"
      ],
      "@xm-ngx/core/environment": [
        "src/environments"
      ],
      "@xm-ngx/ext/*": [
        "src/app/ext/*"
      ],
      "@xm-ngx/alert": [
        "packages/alert"
      ],
      "@xm-ngx/confirm-dialog": [
        "packages/confirm-dialog"
      ],
      "@xm-ngx/json-schema-form": [
        "packages/json-schema-form"
      ],
      "@xm-ngx/json-schema-form/*": [
        "packages/json-schema-form/*"
      ],
      "@xm-ngx/toaster": [
        "packages/toaster"
      ],
      "@xm-ngx/logger": [
        "packages/logger"
      ],
      "@xm-ngx/logger/testing": [
        "packages/logger/testing"
      ],
      "@xm-ngx/timeline": [
        "packages/timeline"
      ],
      "@xm-ngx/timeline/*": [
        "packages/timeline/*"
      ],
      "@xm-ngx/shared": [
        "packages/shared"
      ],
      "@xm-ngx/shared/exceptions": [
        "packages/shared/exceptions"
      ],
      "@xm-ngx/shared/validators": [
        "packages/shared/validators"
      ],
      "@xm-ngx/shared/interfaces": [
        "packages/shared/interfaces"
      ],
      "@xm-ngx/shared/operators": [
        "packages/shared/operators"
      ],
      "@xm-ngx/shared/operators/*": [
        "packages/shared/operators/*"
      ],
      "@xm-ngx/translation": [
        "packages/translation"
      ],
      "@xm-ngx/translation/testing": [
        "packages/translation/testing"
      ],
      "@xm-ngx/account": [
        "packages/account"
      ],
      "@xm-ngx/account/*": [
        "packages/account/*"
      ],
      "@xm-ngx/entity": [
        "packages/entity"
      ],
      "@xm-ngx/entity/*": [
        "packages/entity/src/*"
      ],
      "@xm-ngx/dynamic": [
        "packages/dynamic"
      ],
      "@xm-ngx/dynamic/*": [
        "packages/dynamic/*"
      ],
      "@xm-ngx/dashboard": [
        "packages/dashboard"
      ],
      "@xm-ngx/dashboard/testing": [
        "packages/dashboard/testing"
      ],
      "@xm-ngx/balance": [
        "packages/balance"
      ],
      "@xm-ngx/balance/*": [
        "packages/balance/src/*"
      ],
      "rxjs": [
        "node_modules/rxjs"
      ],
      "rxjs/*": [
        "node_modules/rxjs/*"
      ],
      "@angular/*": [
        "node_modules/@angular/*"
      ]
    },
    "inlineSources": true,
    "module": "es2020",
    "stripInternal": false,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "newLine": "lf",
    "strictFunctionTypes": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true
  }
}
