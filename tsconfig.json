{
  "compileOnSave": true,
  "compilerOptions": {
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "baseUrl": "./",
    "sourceMap": true,
    "declaration": true,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@xm-ngx/styles": ["packages/styles"],
      "@xm-ngx/administration/*": ["packages/administration/src/*"],
      "@xm-ngx/components/*": ["packages/components/src/*"],
      "@xm-ngx/core": ["packages/core"],
      "@xm-ngx/core/auth": ["src/app/shared/auth"],
      "@xm-ngx/core/config": ["packages/core/config"],
      "@xm-ngx/core/user": ["packages/core/user"],
      "@xm-ngx/core/permission": ["src/app/shared/privilege"],
      "@xm-ngx/core/theme": ["packages/xm-theme/public_api"],
      "@xm-ngx/core/environment": ["src/environments"],
      "@xm-ngx/ext/*": ["src/app/ext/*"],
      "@xm-ngx/alert": ["packages/xm-alert"],
      "@xm-ngx/json-scheme-form": ["src/app/shared/jsf-extention"],
      "@xm-ngx/json-schema-form/*": ["packages/json-schema-form/*"],
      "@xm-ngx/toaster": ["src/app/shared/toaster"],
      "@xm-ngx/shared": ["src/app/modules/xm-shared"],
      "@xm-ngx/shared/validators": ["packages/shared/validators"],
      "@xm-ngx/shared/interfaces": ["packages/shared/interfaces"],
      "@xm-ngx/shared/operators": ["packages/shared/operators"],
      "@xm-ngx/translation": ["packages/xm-translation/src"],
      "@xm-ngx/entity": ["src/app/xm-entity/shared"],
      "@xm-ngx/account/*": ["packages/xm-account/src/*"],
      "@xm-ngx/entity/*": ["src/app/xm-entity/*"],
      "@xm-ngx/dynamic": ["packages/dynamic"],
      "@xm-ngx/dashboard": ["src/app/xm-dashboard"],
      "@xm-ngx/xm-balance": ["src/app/xm-balance"],
      "@xm-ngx/components/language": ["src/app/shared/language"]

    },
    "inlineSources": true,
    "module": "esnext",
    "stripInternal": false,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "newLine": "lf",
    "strictFunctionTypes": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true
  }
}
