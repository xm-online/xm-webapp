{
  "compileOnSave": true,
  "compilerOptions": {
    "forceConsistentCasingInFileNames": false,
    "strict": false,
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "baseUrl": "./",
    "sourceMap": true,
    "declaration": true,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "es2017",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@xm-ngx/styles": ["packages/styles"],
      "@xm-ngx/administration/*": ["packages/administration/src/*"],
      "@xm-ngx/components/*": ["packages/components/*"],
      "@xm-ngx/core": ["packages/core"],
      "@xm-ngx/core/auth": ["src/app/shared/auth"],
      "@xm-ngx/core/config": ["packages/core/config"],
      "@xm-ngx/core/config/testing": ["packages/core/config/testing"],
      "@xm-ngx/core/user": ["packages/core/user"],
      "@xm-ngx/core/user/testing": ["packages/core/user/testing"],
      "@xm-ngx/core/global-error-handler": ["packages/core/global-error-handler"],
      "@xm-ngx/core/permission": ["src/app/shared/privilege"],
      "@xm-ngx/core/permission/testing": ["packages/core/permission/testing"],
      "@xm-ngx/core/theme": ["packages/theme/public_api"],
      "@xm-ngx/core/environment": ["src/environments"],
      "@xm-ngx/ext/*": ["src/app/ext/*"],
      "@xm-ngx/alert": ["packages/alert"],
      "@xm-ngx/json-scheme-form": ["src/app/shared/jsf-extention"],
      "@xm-ngx/json-schema-form/*": ["packages/json-schema-form/*"],
      "@xm-ngx/toaster": ["src/app/shared/toaster"],
      "@xm-ngx/logger": ["packages/logger"],
      "@xm-ngx/logger/testing": ["packages/logger/testing"],
      "@xm-ngx/shared": ["src/app/modules/xm-shared"],
      "@xm-ngx/shared/exceptions": ["packages/shared/exceptions"],
      "@xm-ngx/shared/validators": ["packages/shared/validators"],
      "@xm-ngx/shared/interfaces": ["packages/shared/interfaces"],
      "@xm-ngx/shared/operators": ["packages/shared/operators"],
      "@xm-ngx/translation": ["packages/translation"],
      "@xm-ngx/entity": ["src/app/xm-entity/shared"],
      "@xm-ngx/account/*": ["packages/account/src/*"],
      "@xm-ngx/entity/*": ["src/app/xm-entity/*"],
      "@xm-ngx/dynamic": ["packages/dynamic"],
      "@xm-ngx/dynamic/route": ["packages/dynamic/route"],
      "@xm-ngx/dashboard": ["packages/dashboard/src"],
      "@xm-ngx/dashboard/testing": ["packages/dashboard/testing"],
      "@xm-ngx/xm-balance": ["src/app/xm-balance"],
      "@xm-ngx/components/language": ["src/app/shared/language"]

    },
    "inlineSources": true,
    "module": "es2020",
    "stripInternal": false,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "newLine": "lf",
    "strictFunctionTypes": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true
  }
}
