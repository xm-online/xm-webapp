{
  "compileOnSave": true,
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": false,
    "strict": false,
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "baseUrl": "./",
    "sourceMap": true,
    "declaration": true,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "es2020",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@xm-ngx/administration/*": [
        "packages/administration/src/*"
      ],
      "@xm-ngx/core/environment": [
        "src/environments"
      ],
      "@xm-ngx/ext/*": [
        "src/app/ext/*"
      ],
      "@xm-ngx/confirm-dialog": [
        "packages/confirm-dialog"
      ],
      "@xm-ngx/json-schema-form/core": [
        "packages/json-schema-form/core"
      ],
      "@xm-ngx/json-schema-form/components": [
        "packages/json-schema-form/components"
      ],
      "@xm-ngx/toaster": [
        "packages/toaster"
      ],
      "@xm-ngx/logger": [
        "packages/logger"
      ],
      "@xm-ngx/logger/testing": [
        "packages/logger/testing"
      ],
      "@xm-ngx/timeline": [
        "packages/timeline"
      ],
      "@xm-ngx/timeline/*": [
        "packages/timeline/*"
      ],
      "@xm-ngx/shared": [
        "src/app/shared/shared.module.ts"
      ],
      "@xm-ngx/xm-shared": [
        "src/app/shared"
      ],
      "@xm-ngx/xm-shared/*": [
        "src/app/shared/*"
      ],
      "@xm-ngx/translation": [
        "packages/translation"
      ],
      "@xm-ngx/translation/testing": [
        "packages/translation/testing"
      ],
      "@xm-ngx/account": [
        "packages/account"
      ],
      "@xm-ngx/account/*": [
        "packages/account/*"
      ],
      "@xm-ngx/entity": [
        "packages/entity"
      ],
      "@xm-ngx/entity/*": [
        "packages/entity/src/*"
      ],
      "@xm-ngx/dynamic": [
        "packages/dynamic"
      ],
      "@xm-ngx/dynamic/*": [
        "packages/dynamic/*"
      ],
      "@xm-ngx/dashboard": [
        "packages/dashboard"
      ],
      "@xm-ngx/dashboard/testing": [
        "packages/dashboard/testing"
      ],
      "@xm-ngx/balance/*": [
        "packages/balance/src/*"
      ],
    },
    "inlineSources": true,
    "module": "es2020",
    "stripInternal": false,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "newLine": "lf",
    "strictFunctionTypes": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true
  }
}
