{
  "compileOnSave": true,
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": false,
    "strict": false,
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "baseUrl": "./",
    "sourceMap": true,
    "declaration": true,
    "downlevelIteration": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "ES2022",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@xm-ngx/core/environment": [
        "src/environments"
      ],
      "@xm-ngx/ext/*": [
        "src/app/ext/*"
      ],
      "@xm-ngx/confirm-dialog": [
        "node_modules/@xm-ngx/components/confirm-dialog"
      ],
      "@xm-ngx/entity/card": [
        "node_modules/@xm-ngx/entity/card",
      ],
      "@xm-ngx/entity/search": [
        "node_modules/@xm-ngx/entity/search",
      ],
      "@xm-ngx/entity/pipes": [
        "node_modules/@xm-ngx/entity/pipes",
      ],
      "@xm-ngx/entity/*": [
        "node_modules/@xm-ngx/entity",
      ],
      "@xm-ngx/shared/helpers": [
        "node_modules/@xm-ngx/operators",
      ],
      "@xm-ngx/json-schema-form/core": [
        "node_modules/@xm-ngx/json-schema-form"
      ],
      "@xm-ngx/json-schema-form/components": [
        "node_modules/@xm-ngx/json-schema-form"
      ],
      "@xm-ngx/json-schema-form/components/*": [
        "node_modules/@xm-ngx/json-schema-form"
      ],
      "@xm-ngx/shared/exceptions": [
        "node_modules/@xm-ngx/exceptions"
      ],
      "@xm-ngx/shared/validators": [
        "node_modules/@xm-ngx/validators"
      ],
      "@xm-ngx/shared/interfaces": [
        "node_modules/@xm-ngx/interfaces"
      ],
      "@xm-ngx/shared/operators": [
        "node_modules/@xm-ngx/operators"
      ],
      "@xm-ngx/shared/pipes": [
        "node_modules/@xm-ngx/pipes"
      ],
      "@xm-ngx/components/entity-collection": [
        "node_modules/@xm-ngx/repositories"
      ],
      "@xm-ngx/entity/xm-mat-card": [
        "node_modules/@xm-ngx/entity/card"
      ],
      "rxjs": [
        "node_modules/rxjs"
      ],
      "rxjs/*": [
        "node_modules/rxjs/*"
      ],
      "@angular/*": [
        "node_modules/@angular/*"
      ]
    },
    "inlineSources": true,
    "module": "es2020",
    "stripInternal": false,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "noImplicitReturns": true,
    "newLine": "lf",
    "strictFunctionTypes": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "useDefineForClassFields": false
  }
}
